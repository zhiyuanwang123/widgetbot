{
  "name": "widgetbot",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "pull": "git stash && git pull && yarn && yarn build",
    "postinstall": "yarn patch",
    "precommit": "pretty-quick --staged",
    "patch": "patch -i patches/** -p0 -s -r /dev/null --no-backup-if-mismatch > /dev/null"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "lerna": "^3.4.0"
  }
}
