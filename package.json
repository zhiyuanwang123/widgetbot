{
  "name": "embed",
  "main": "src/index.tsx",
  "version": "0.1.0",
  "private": true,
  "homepage": "/channels/",
  "proxy": {
    "/api": {
      "target": "http://localhost:7000",
      "changeOrigin": true,
      "secure": false,
      "ws": true
    }
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "pull": "git stash && git pull && yarn && yarn build",
    "precommit": "pretty-quick --staged",
    "schema": "apollo schema:download --endpoint=http://localhost:7000/api/graphql src/controllers/apollo/codegen/schema.json",
    "codegen": "apollo codegen:generate --target=typescript --schema=\"src/controllers/apollo/codegen/schema.json\" --queries=\"src/**/*.{ts,tsx}\" --addTypename",
    "codegen:live": "yarn codegen --schema=\"http://localhost:7000/api/graphql\"",
    "doc": "typedoc --includeDeclarations --excludeExternals --externalPattern \"**/node_modules/!(@widgetbot)/**\" --ignoreCompilerErrors --out ./docs/ ./src"
  },
  "dependencies": {
    "@cerebral/fluent": "^1.0.3",
    "@cerebral/react": "^3.2.2",
    "@cerebral/router": "^3.2.2",
    "@cerebral/storage": "^4.2.2",
    "@types/color": "^3.0.0",
    "@types/highlight.js": "^9.12.3",
    "@types/memoizee": "^0.4.2",
    "@types/ramda": "^0.25.34",
    "@types/raven-js": "^3.10.0",
    "@types/rc-tooltip": "^3.4.11",
    "@types/react-custom-scrollbars": "^4.0.4",
    "@types/react-intl": "^2.3.8",
    "@types/react-notification-system": "^0.2.37",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-virtualized": "^9.18.4",
    "@types/recompose": "^0.26.1",
    "@types/socket.io-client": "^1.4.32",
    "@widgetbot/embed-api": "^1.1.3",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.3.5",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-logger": "^1.2.2",
    "apollo-link-persisted-queries": "^0.2.1",
    "apollo-link-retry": "^2.2.4",
    "apollo-link-state": "^0.4.1",
    "apollo-link-ws": "^1.0.8",
    "cerebral": "4.2.2",
    "color": "^3.0.0",
    "emotion": "^9.2.5",
    "emotion-theming": "^9.2.5",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "highlight.js": "^9.12.0",
    "match-sorter": "^2.2.3",
    "memoizee": "^0.4.12",
    "moment": "^2.22.2",
    "ramda": "^0.25.0",
    "rc-tooltip": "^3.7.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.4.1",
    "react-easy-emoji": "^1.2.0",
    "react-emotion": "^9.2.5",
    "react-icons": "^2.2.7",
    "react-intl": "^2.4.0",
    "react-notification-system": "^0.2.17",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-transition-group": "^2.4.0",
    "react-virtualized": "^9.20.1",
    "recompose": "^0.27.1",
    "scroll-into-view-if-needed": "^2.2.11",
    "simple-markdown": "^0.4.1",
    "socket.io-client": "^2.1.1",
    "subscriptions-transport-ws": "^0.9.12"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/node": "10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "husky": "^0.14.3",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "ts-graphql-plugin": "^1.1.2",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.0-dev.20180710"
  }
}
